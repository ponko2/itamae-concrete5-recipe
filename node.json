{
  "role": "web",
  "apache2": {
    "sites": [{
      "name": "concrete5",
      "server_name": "www.concrete5.app",
      "docroot": "/var/www/concrete5",
      "allow_override": [
        "FileInfo",
        "Options"
      ]
    }]
  },
  "mysql": {
    "server_root_password": "",
    "allow_remote_root": true,
    "databases": [{
      "name": "concrete5",
      "charset": "utf8",
      "collate": "utf8_general_ci",
      "privileges": [{
        "types": ["ALL PRIVILEGES"],
        "user": "concrete5",
        "host": "localhost",
        "password": ""
      }]
    }]
  },
  "concrete5": {
    "download_url": "http://www.concrete5.org/download_file/-/view/79252/",
    "version": "5.7.4.2",
    "install_path": "/var/www/concrete5"
  }
}
